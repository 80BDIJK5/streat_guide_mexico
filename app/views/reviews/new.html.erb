<div class="container">
  <div class="row justify-content-center">
    <div class="col-8">
      <h1>ADD A NEW FOOD STALL</h1>
        <%= simple_form_for [ @stall, @review ] do |f| %>
          <%= f.input :title %>
          <%= f.input :comment %>
          <%=f.input :rating, input_html: { type: 'number' } %>
          <%= link_to "Submit a review", stall_path(@stall), class: 'btn btn-block btn-secondary' %>
        <% end %>
    </div>
  </div>
</div>




