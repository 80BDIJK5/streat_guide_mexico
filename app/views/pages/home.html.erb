<%= render 'shared/banner' %>

<div class="row">
  <div class="col-sm-8 p-5">
  <h1>Trending today</h1>
    <div class="card-container">
      <% 2.times do |time| %>
      <%= render partial: "shared/stallcard" %>
    <% end %>
    </div>
  </div>

  <div class="col-sm-4 p-5">
  <h1>Latest reviews</h1>
    <% 2.times do |time| %>
      <%= render partial: "shared/review-card" %>
    <% end %>
  </div>
</div>
