<div class="blog-card">
 <div class="meta">
  <% if stall.photos.attached? %>
  <div class="photo-info">
   <%= cl_image_tag stall.photos.first.key, style: 'object-fit: cover; height: 100%; width: 100%'%>
 </div>
  <% else %>
  <img src="https://images.unsplash.com/photo-1599974579688-8dbdd335c77f?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1542&q=80" object-fit= cover width= 100%, height= 100% />
  <% end %>
</div>
<%= link_to stall_path(stall), style: 'text-decoration: none; link-style: none; color: black;' do %>
    <div class="description">
      <h2><%= stall.name %></h2>
        <% end %>
    </div>
  </div>
