

<div class="blog-card">
 <div class="meta">
  <% if stall.photos.attached? %>
  <div class="photo">
   <%= cl_image_tag stall.photos.first.key, crop: :fill, style: 'object-fit: cover;', height: '140px', width: '146px' %>
  <% else %>
  <img src="https://images.unsplash.com/photo-1599974579688-8dbdd335c77f?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=1542&q=80" object-fit= cover width= 110px, height= 146px />
  <% end %>
 </div>
    <div class="description">
      <h1><%= stall.name %></h1>
      <h2>Direction: <%= stall.address %></h2>
    </div>
  </div>
